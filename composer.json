{
    "name": "baldwin/magento2-module-less-js-compiler",
    "description": "Allows Magento 2 to compile less files using the less nodejs compiler",
    "type": "magento2-module",
    "keywords": ["magento", "magento2", "less", "compiler"],
    "homepage": "https://github.com/baldwin-agency/magento2-module-less-js-compiler",
    "license": "MIT",
    "authors": [
        {
            "name": "Pieter Hoste",
            "email": "pieter@baldwin.be",
            "role": "Problem Solver"
        }
    ],
    "require": {
        "magento/framework": "^100.0.9 || ^101.0.0 || ^102.0.0",
        "magento/module-developer": "^100.0.5",
        "magento/magento-composer-installer": "^0.1.9",
        "php": "~5.5.0|~5.6.0|7.0.2|7.0.4|~7.0.6|~7.1.0|~7.2.0"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Baldwin\\LessJsCompiler\\": ""
        }
    }
}
